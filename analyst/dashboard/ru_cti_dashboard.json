      "targets": [
        {
          "query": "import \"experimental\"\n\n// 1. Get Base Data\nbaseData = from(bucket: \"ransomware_tracker\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"attack_intel\")\n  |> filter(fn: (r) => r[\"_field\"] == \"src_lat\" or r[\"_field\"] == \"src_lon\" or r[\"_field\"] == \"dst_lat\" or r[\"_field\"] == \"dst_lon\" or r[\"_field\"] == \"victims\")\n  // Force Value to String to allow pivot\n  |> map(fn: (r) => ({ r with _value: string(v: r._value) }))\n  |> pivot(rowKey:[\"_time\"], columnKey: [\"_field\"], valueColumn: \"_value\")\n  // Filter for valid destinations\n  |> filter(fn: (r) => exists r.dst_lat and exists r.dst_lon)\n  |> map(fn: (r) => ({r with trace_id: r.gang + \"_\" + string(v: r._time)}))\n\n// 2. Format Source (Attacker)\nsources = baseData\n  |> map(fn: (r) => ({\n      _time: r._time,\n      trace_id: r.trace_id,\n      \"Attacker\": r.gang,\n      \"Victim\": r.victims,\n      // Cast string -> float\n      latitude: float(v: r.src_lat),\n      longitude: float(v: r.src_lon),\n      point_type: \"Attacker\"\n  }))\n\n// 3. Format Destination (Victim)\ndestinations = baseData\n  |> map(fn: (r) => ({\n      _time: experimental.addDuration(d: 1s, to: r._time),\n      trace_id: r.trace_id,\n      \"Attacker\": r.gang,\n      \"Victim\": r.victims,\n      // Cast string -> float\n      latitude: float(v: r.dst_lat),\n      longitude: float(v: r.dst_lon),\n      point_type: \"Victim\"\n  }))\n\n// 4. Union\nunion(tables: [sources, destinations])\n  |> group(columns: [\"trace_id\"])\n  |> sort(columns: [\"_time\"], desc: false)\n  // Ensure we keep the numeric columns visible\n  |> keep(columns: [\"_time\", \"trace_id\", \"latitude\", \"longitude\", \"Attacker\", \"Victim\"])",
          "refId": "A"
        }
      ],
